{
  "version": 2,
  "builds": [
    {
      "src": "app/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/app/$1.js"
    }
  ]
}
